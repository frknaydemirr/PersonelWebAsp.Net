                                        </div>
                                        <h2 class="title split-collab tmp-scroll-trigger tmp-fade-in animation-order-2">
                                            @Model.Resources.FirstOrDefault(x => x.NavigationName == "İletisimTabloDetay").Value
                                        </h2>
                                    </div>
                                </div>
                                <div class="col-lg-7">
                                    <div class="contact-inner">
                                        <div class="contact-form">
                                            <div id="form-messages" class="error"></div>

                                            <form class="tmp-dynamic-form" id="contact-form" method="POST" action="#">
                                                <div class="contact-form-wrapper row">

                                                    <!-- Ad -->
                                                    <div class="col-lg-6">
                                                        <div class="form-group">
                                                            <input class="input-field" name="name" id="contact-name"
                                                                   placeholder="@Model.Resources.FirstOrDefault(x => x.NavigationName == "İsimDetay").Value"
                                                                   type="text" required>
                                                        </div>
                                                    </div>

                                                    <!-- Telefon -->
                                                    <div class="col-lg-6">
                                                        <div class="form-group">
                                                            <input class="input-field" id="contact-phone" name="phoneNumber"
                                                                   placeholder="@Model.Resources.FirstOrDefault(x => x.NavigationName == "TelefonDetay").Value"
                                                                   type="tel" required>
                                                        </div>
                                                    </div>

                                                    <!-- Email -->
                                                    <div class="col-lg-6">
                                                        <div class="form-group">
                                                            <input class="input-field" id="contact-email" name="email"
                                                                   placeholder="@Model.Resources.FirstOrDefault(x => x.NavigationName == "EmailDetay").Value"
                                                                   type="email" required>
                                                        </div>
                                                    </div>

                                                    <!-- Konu -->
                                                    <div class="col-lg-6">
                                                        <div class="form-group">
                                                            <input class="input-field" type="text" id="subject" name="subject"
                                                                   placeholder="@Model.Resources.FirstOrDefault(x => x.NavigationName == "KonuDetay").Value">
                                                        </div>
                                                    </div>

                                                    <!-- Mesaj -->
                                                    <div class="col-lg-12">
                                                        <div class="form-group">
                                                            <textarea class="input-field"
                                                                      placeholder="@Model.Resources.FirstOrDefault(x => x.NavigationName == "MesajDetay").Value"
                                                                      name="message" id="contact-message" required></textarea>
                                                        </div>
                                                    </div>

                                                    <!-- Gönder Butonu -->
                                                    <div class="col-lg-12">
                                                        <div class="tmp-button-here">
                                                            <button class="tmp-btn hover-icon-reverse radius-round w-100"
                                                                    type="button" id="gonder" name="gonder">
