    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

    <script type="text/javascript">
    $(document).ready(function () {
        $("#gonder").click(function (e) {
            debugger
            e.preventDefault();

            var name = $("#contact-name").val().trim();
            var phoneNumber = $("#contact-phone").val().trim();
            var email = $("#contact-email").val().trim();
            var subject = $("#subject").val().trim();
            var message = $("#contact-message").val().trim();

            // Zorunlu alan kontrolü
            if (!name || !email || !message) {
                Swal.fire({
                    icon: 'error',
                    title: 'Hata!',
                    text: 'Lütfen zorunlu alanları (Ad, Email, Mesaj) doldurunuz.'
                });
                return;
            }

            $.ajax({
                url: '@Url.Action("SendMessage", "Home")',
                type: 'POST',
                dataType: 'json',
                data: {
                    name: name,
                    email: email,
                    phoneNumber: phoneNumber,
                    subject: subject,
                    message: message,
                    langId: currentDilId
                },
                success: function (data) {
                    if (data.success) {
                        Swal.fire({
                            icon: 'success',
                            title: 'Gönderildi!',
                            text: 'Mesajınız başarıyla gönderildi. En kısa sürede size dönüş yapılacaktır.',
                            confirmButtonText: 'Tamam'
                        });

                        // Formu temizle
                        $("#contact-form")[0].reset();
                    } else {
                        Swal.fire({
                            icon: 'error',
                            title: 'Hata!',
                            text: data.message
                        });
                    }
                },
                error: function (xhr, status, error) {
                    console.error("AJAX hata:", error);
                    Swal.fire({
                        icon: 'error',
                        title: 'Bir hata oluştu!',
                        text: 'Lütfen tekrar deneyin.'
                    });
                }
            });
        });
    });

    // Formun kendi submit olayını engelle
    $("#contact-form").submit(function (e) {
        e.preventDefault();
    });
    </script>













