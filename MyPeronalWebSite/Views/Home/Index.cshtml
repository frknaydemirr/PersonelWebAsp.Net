    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

    <script type="text/javascript">
$(document).ready(function () {

    // Razor değişkenlerini JS'e aktar
    var emailRegex = new RegExp('@Html.Raw(@"^[a-z0-9._%+-]+@[a-z0-9.-]+\.[a-z]{2,}$")', 'i');
    var phoneRegex = new RegExp('@Html.Raw(@"^[0-9]{10,15}$")');

    $("#gonder").click(function (e) {
        e.preventDefault();

        var name = $("#contact-name").val().trim();
        var phoneNumber = $("#contact-phone").val().trim();
        var email = $("#contact-email").val().trim();
        var subject = $("#subject").val().trim();
        var message = $("#contact-message").val().trim();

        // Zorunlu alan kontrolü
        if (!name || !email || !message) {
            Swal.fire({
                icon: 'error',
                title: 'Hata!',
                text: 'Lütfen zorunlu alanları (Ad, Email, Mesaj) doldurunuz.'
            });
            return;
        }

        // Email regex kontrolü
        if (!emailRegex.test(email)) {
            Swal.fire({
                icon: 'error',
                title: 'Hata!',
                text: 'Lütfen geçerli bir email adresi giriniz.'
            });
            return;
        }

        // Telefon regex kontrolü (opsiyonel)
        if (phoneNumber && !phoneRegex.test(phoneNumber)) {
            Swal.fire({
                icon: 'error',
                title: 'Hata!',
                text: 'Lütfen geçerli bir telefon numarası giriniz (10-15 rakam).'
            });
            return;
        }

        // Ajax ile form gönderimi
        $.ajax({
            url: '@Url.Action("SendMessage", "Home")',
            type: 'POST',
            dataType: 'json',
            data: {
                name: name,
                email: email,
                phoneNumber: phoneNumber,
                subject: subject,
                message: message,
                langId: currentDilId
            },
            success: function (data) {
                if (data.success) {
                    Swal.fire({
                        icon: 'success',
                        title: 'Gönderildi!',
                        text: 'Mesajınız başarıyla gönderildi. En kısa sürede size dönüş yapılacaktır.',
                        confirmButtonText: 'Tamam'
                    });
                    $("#contact-form")[0].reset();
                } else {
                    Swal.fire({
                        icon: 'error',
                        title: 'Hata!',
                        text: data.message
                    });
                }
            },
            error: function (xhr, status, error) {
                console.error("AJAX hata:", error);
                Swal.fire({
                    icon: 'error',
                    title: 'Bir hata oluştu!',
                    text: 'Lütfen tekrar deneyin.'
                });
            }
        });
    });

    // Formun submit olayını engelle
    $("#contact-form").submit(function (e) {
        e.preventDefault();
    });

});
    </script>

