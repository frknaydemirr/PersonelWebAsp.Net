<section class="get-in-touch-area tmp-section-gapBottom">
    <div class="container">
        <div class="contact-get-in-touch-wrap" id="get-in-touch">
            <div class="get-in-touch-wrapper tmponhover" style="--x: 460px; --y: 415px;">
                <div class="row g-5 align-items-center">
                    <div class="col-lg-5">
                        <div class="section-head text-align-left">
                            <div class="section-sub-title tmp-scroll-trigger tmp-fade-in animation-order-1">
                                <span class="subtitle">@Model.Tbl_Resource.FirstOrDefault(x => x.NavigationName == "İletisimeGec").Value</span>
                            </div>
                            <h2 class="title split-collab tmp-scroll-trigger tmp-fade-in animation-order-2">
                                @Model.Tbl_Resource.FirstOrDefault(x => x.NavigationName == "İletisimTablo").Value
                            </h2>
                        </div>
                    </div>
                    <div class="col-lg-7">
                        <div class="contact-inner">
                            <div class="contact-form">
                                <div id="form-messages" class="error"></div>

                                <!-- action değiştirildi -->
                                <form class="tmp-dynamic-form" id="contact-form" method="POST" action="#">
                                    <div class="contact-form-wrapper row">

                                        <div class="col-lg-6">
                                            <div class="form-group">
                                                <input class="input-field" name="name" id="contact-name"
                                                       placeholder="@Model.Tbl_Resource.FirstOrDefault(x => x.NavigationName == "İsim").Value"
                                                       type="text" required="">
                                            </div>
                                        </div>

                                        <div class="col-lg-6">
                                            <div class="form-group">
                                                <!-- name eklendi -->
                                                <input class="input-field" id="contact-phone" name="phoneNumber"
                                                       placeholder="@Model.Tbl_Resource.FirstOrDefault(x => x.NavigationName == "Telefon").Value"
                                                       type="tel" required="">
                                            </div>
                                        </div>

                                        <div class="col-lg-6">
                                            <div class="form-group">
                                                <input class="input-field" id="contact-email" name="email"
                                                       placeholder="@Model.Tbl_Resource.FirstOrDefault(x => x.NavigationName == "Email").Value"
                                                       type="email" required="">
                                            </div>
                                        </div>

                                        <div class="col-lg-6">
                                            <div class="form-group">
                                                <input class="input-field" type="text" id="subject" name="subject"
                                                       placeholder="@Model.Tbl_Resource.FirstOrDefault(x => x.NavigationName == "Konu").Value">
                                            </div>
                                        </div>

                                        <div class="col-lg-12">
                                            <div class="form-group">
                                                <textarea class="input-field"
                                                          placeholder="@Model.Tbl_Resource.FirstOrDefault(x => x.NavigationName == "Mesaj").Value"
                                                          name="message" id="contact-message" required=""></textarea>
                                            </div>
                                        </div>

                                        <div class="col-lg-12">
                                            <div class="tmp-button-here">
                                                <!-- type değiştirildi -->
                                                <button class="tmp-btn hover-icon-reverse radius-round w-100"
                                                        type="submit" id="gonder" name="gonder">
                                                    <span class="icon-reverse-wrapper">
                                                        <span class="btn-text">
                                                            @Model.Tbl_Resource.FirstOrDefault(x => x.NavigationName == "Gonder").Value
                                                        </span>
                                                        <span class="btn-icon"><i class="fa-sharp fa-regular fa-arrow-right"></i></span>
                                                        <span class="btn-icon"><i class="fa-sharp fa-regular fa-arrow-right"></i></span>
                                                    </span>
                                                </button>
                                            </div>
                                        </div>

                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

