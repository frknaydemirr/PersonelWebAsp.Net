﻿
@model MyPeronalWebSite.Models.ViewModel.IndexViewModel
@using MyPeronalWebSite.Helper
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<!-- tpm-header-area start -->
<!-- tpm-header-area end -->



<div class="d-block d-xl-none">
    <div class="tmp-popup-mobile-menu">
        <div class="inner">
            <div class="header-top">
                <div class="logo">
                    <a href="index.html" class="logo-area">

                        <img class="logo-white" src="/content/images/logo/logo-white.png" alt="Reeni - Personal Portfolio HTML Template for developers and freelancers">
                    </a>

                </div>
                <div class="close-menu">
                    <button class="close-button tmp-round-action-btn">
                        <i class="fa-sharp fa-light fa-xmark"></i>
                    </button>
                </div>
            </div>
            @*mobil*@
            @{

                var currentUrl = Context.Request.Path.ToString();
                var menuItems = Model.Tbl_Navbar ?? new List<MyPeronalWebSite.Models.VT.Tbl_Navbar>();
            }

            <ul class="tmp-mainmenu">
                @foreach (var item in menuItems)
                {
                    var isActive = currentUrl == item.URL ? "current" : "";
                    <li class="@isActive">
                        <a href="@item.URL">@item.Title</a>
                    </li>
                }
            </ul>




            <div class="social-wrapper mt--40">

                <div class="social-link">
                    <a href="https://www.linkedin.com/in/furkanaydemir13/" target="_blank"><i class="fa-brands fa-linkedin-in"></i></a>

                    <a href="https://github.com/frknaydemirr" target="_blank"><i class="fa-brands fa-github"></i></a>
                </div>
            </div>
            <!-- social area end -->



        </div>
    </div>
</div>



<!-- tmp banner area start -->
<div class="rpp-banner-three-area">
    <div class="container">
        <div class="banner-three-main-wrapper">
            <div class="row">
                <div class="col-lg-4">
                    <div class="inner">
                        <span class="sub-title tmp-scroll-trigger tmp-fade-in animation-order-1"> @Model.Tbl_Resource.FirstOrDefault(x => x.NavigationName == "Giriş").Value</span>
                        <h1 class="title tmp-scroll-trigger tmp-fade-in animation-order-2">Furkan Aydemir</h1>
                        <div class="button-area-banner-three tmp-scroll-trigger tmp-fade-in animation-order-3">


                            <a class="tmp-btn hover-icon-reverse radius-round" href="@Model.Tbl_AboutMe.Cv" download>
                                <span class="icon-reverse-wrapper">
                                    <span class="btn-text">@Model.Tbl_Resource.FirstOrDefault(x => x.NavigationName == "Cv").Value</span>
                                    <span class="btn-icon"><i class="fa-sharp fa-regular fa-download"></i></span>
                                    <span class="btn-icon"><i class="fa-sharp fa-regular fa-download"></i></span>
                                </span>
                            </a>


                        </div>
                    </div>
                </div>
                <div class="col-lg-3">
                    <div class="banner-right-content">
                        <div class="about-me tmp-scroll-trigger tmp-fade-in animation-order-1">
                            <h3 class="title"> @Model.Tbl_Resource.FirstOrDefault(x => x.NavigationName == "HakkımdaAna").Value</h3>
                            <p class="para tmp-title-split">

                                @Html.Raw(@Model.Tbl_AboutMe.Description)
                            </p>
                        </div>
                        <div class="find-me-on mt--40 tmp-scroll-trigger tmp-fade-in animation-order-2">
                            <h2 class="find-me-on-title"> @Model.Tbl_Resource.FirstOrDefault(x => x.NavigationName == "Bulmak").Value</h2>
                            <div class="social-link banner">

                                <a href="@Model.Tbl_AboutMe.LinkedlnURL" target="_blank"><i class="fa-brands fa-linkedin-in"></i></a>
                                <a href="@Model.Tbl_AboutMe.GithubURL" target="_blank"><i class="fa-brands fa-github"></i></a>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="bg-benner-img-three">

                <img class="tmp-scroll-trigger tmp-zoom-in animation-order-2" src="@Model.Tbl_AboutMe.ImageURL" alt="banner-img-3">


            </div>
            <h2 class="texts-one up-down-2"> @Model.Tbl_Resource.FirstOrDefault(x => x.NavigationName == "WebTasarım").Value</h2>
            <h2 class="texts-two up-down">@Model.Tbl_Resource.FirstOrDefault(x => x.NavigationName == "WebTasarım").Value</h2>
        </div>
    </div>
</div>
<!-- tmp banner area end -->
<!-- Tpm Service Area Start -->
<section class="service-area tmp-section-gap">
    <div class="container">
        <div class="row justify-content-center">
            @foreach (var item in Model.Tbl_Skills)
            {
                <div class="col-lg-3 col-md-4 col-sm-6">
                    <div class="service-card-v1 tmp-scroll-trigger tmp-fade-in animation-order-1 tmp-link-animation">

                        <div class="service-card-icon">

                            <i class="@item.Icon"></i>
                        </div>
                        <h4 class="service-title"><a href="">@item.WhichSkills</a></h4>

                    </div>
                </div>
            }
        </div>

    </div>

</section>
<!-- Tpm Service Area End -->
<!-- Tpm Counter Area Start -->
<!-- Tpm Counter Area End -->
<!-- Tpm Latest Service Area Start -->
<section class="latest-service-area tmp-section-gapTop">
    <div class="container" id="About">
        <div class="section-head mb--60">
            <div class="section-sub-title center-title tmp-scroll-trigger tmp-fade-in animation-order-1">
                <span class="subtitle">@Model.Tbl_Resource.FirstOrDefault(x => x.NavigationName == "BenKimim").Value</span>
            </div>


        </div>
        <div class="row">
            <div class="col-lg-6">
                <div class="service-card-v2 tmponhover tmp-scroll-trigger tmp-fade-in animation-order-1">
                    <h2 class="service-card-num"><span>1.</span>@Model.Tbl_Resource.FirstOrDefault(x => x.NavigationName == "Kısaca").Value </h2>
                    <p class="service-para">
                        @Model.Tbl_AboutMe.ShortDescription
                    </p>
                </div>

            </div>
            <div class="col-lg-6">
                <div class="service-card-user-image">

                    <img class="tmp-scroll-trigger tmp-zoom-in animation-order-1" src="@Model.Tbl_AboutMe.ImageURL" alt="latest-user-image">


