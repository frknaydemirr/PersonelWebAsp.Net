<!-- Tpm Our Supported Company Area Start -->
<div class="our-supported-company-area tmp-section-gap">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-xl-3 col-lg-3 col-md-3 col-sm-6">
                <h3 class="tech-title">
                    Kulla
                    nılan Teknolojiler
                </h3>
                <div class="support-company-logo tmp-scroll-trigger tmp-fade-in animation-order-1">
                    @foreach (var item in Model.Tbl_Technologies)
                    {
                        <img class="tech-logo" src="@item.ImageURL" alt="Reeni - Personal Portfolio HTML Template" />
                    }
                </div>
            </div>


        </div>
    </div>
</div>
<style>
    .tech-title {
        font-size: 22px;
        font-weight: 600;
        margin-bottom: 15px;
        color: #fff; /* koyu temada beyaz yazı */
        text-align: center; /* ortalı başlık */
        letter-spacing: 1px;
    }

    .support-company-logo {
        display: flex;
        flex-wrap: wrap;
        justify-content: center; /* logolar ortalansın */
        gap: 12px; /* logolar arası boşluk */
    }

    .tech-logo {
        width: 70px;
        height: 70px;
        object-fit: cover;
    }

    .img-box.v2 {
        width: 100%;
        height: 250px; /* parent’ın yüksekliği varsa, resim tamamen kaplar */
        overflow: hidden;
        display: flex; /* hizalama için */
    }
</style>
<!-- Tpm Our Supported Company Area End -->
<!-- Tpm My Price plan Start -->


<!-- Tpm My Price plan End -->
<!-- Tpm Testimonial Area Start -->
<!-- Tpm Testimonial Area End -->
<!-- Tpm Blog and news Area Start -->
<section class="blog-and-news-are tmp-section-gap">
    <div class="container">
        <div class="section-head mb--50" id="projects">
            <div class="section-sub-title center-title tmp-scroll-trigger tmp-fade-in animation-order-1">
                <span class="subtitle"> @Model.Tbl_Resource.FirstOrDefault(x => x.NavigationName == "Bitmiş Proje").Value </span>
            </div>
            <h2 class="title split-collab tmp-scroll-trigger tmp-fade-in animation-order-2">@Model.Tbl_Resource.FirstOrDefault(x => x.NavigationName == "Projeler").Value</h2>
        </div>
        <div class="row">
            @foreach (var item in Model.Tbl_Projects)
            {
                <div class="col-lg-6 col-md-6">
                    <div class="latest-portfolio-card tmp-hover-link tmp-scroll-trigger tmp-fade-in animation-order-1">
                        <div class="portfoli-card-img">
                            <div class="img-box v2">
                                <a title="projectdetail"
                                   href="@Url.Action("ProjectDetail", "Home", new {
                                       title = Url.FriendlyURLTitle(item.ProjectTitle),
                                       id = item.ID
                                   })">
                                    <img class=" hidden-on-mobile" src="@item.ProjectImg" alt="@item.ProjectTitle" />
                                </a>
                            </div>
                        </div>
                        <div class="portfolio-card-content-wrap">
                            <div class="content-left">
                                <h3 class="portfolio-card-title">
                                    <a class="link" title="projectdetail"
                                       href="@Url.Action("ProjectDetail", "Home", new {
                                           title = Url.FriendlyURLTitle(item.ProjectTitle),
                                           id = item.ID
                                       })">
                                        @item.ProjectTitle
                                    </a>
                                </h3>
                                <p class="portfoli-card-para">@item.ProjectEntryDescription</p>
                            </div>
                            <a title="projectdetail"
                               href="@Url.Action("ProjectDetail", "Home", new {
                                   title = Url.FriendlyURLTitle(item.ProjectTitle),
                                   id = item.ID
                               })" class="tmp-arrow-icon-btn">
                                <div class="btn-inner">
                                    <i class="tmp-icon fa-solid fa-arrow-up-right"></i>
                                    <i class="tmp-icon-bottom fa-solid fa-arrow-up-right"></i>
                                </div>
                            </a>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
</section>



